{
  "name": "dll-cli",
  "version": "0.7.2",
  "description": "A CLI tool for Digital Law Lab providing necessary DA package management functionalities for testing (i.e. pushing to Docassemble's playground) and other purposes",
  "bin": {
    "dll-cli": "./dist/index.js"
  },
  "files": [
    "dist/**/*"
  ],
  "type": "module",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc && yarpm-pnpm run copy",
    "copy": "ncp src/python-scripts dist/python-scripts",
    "test": "ts-node src/listr2test.ts"
  },
  "keywords": [
    "cli",
    "Docassemble",
    "DLL",
    "Digital Law Lab"
  ],
  "author": "Sirage_t",
  "license": "MIT",
  "dependencies": {
    "chalk": "^5.2.0",
    "execa": "^6.1.0",
    "fast-fuzzy": "^1.12.0",
    "fs-extra": "^11.1.0",
    "hyperlinker": "^1.0.0",
    "inquirer": "^9.1.4",
    "inquirer-autocomplete-prompt": "^3.0.0",
    "listr2": "^5.0.7",
    "ora": "^6.1.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.32",
    "@swc/helpers": "^0.4.14",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^9.0.3",
    "@types/inquirer-autocomplete-prompt": "^3.0.0",
    "@types/node": "^18.11.18",
    "ncp": "^2.0.0",
    "prettier": "^2.8.3",
    "regenerator-runtime": "^0.13.11",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "yarpm": "^1.2.0"
  }
}
